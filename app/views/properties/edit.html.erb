<%= form_for(@property) do |f| %>

  <div class="field">
    <%= f.label :property_name %>
    <%= f.text_field :property_name %>
  </div>

  <div class="field">
    <%= f.label :fee %>
    <%= f.number_field :fee %>
  </div>

  <div class="field">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div class="field">
    <%= f.label :build_age %>
    <%= f.number_field :building_age %>
  </div>

  <div class="field">
    <%= f.label :remarks %>
    <%= f.text_area :remarks %>
  </div>




  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
